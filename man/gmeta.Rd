\alias{gmeta}
\alias{gmeta.default}
\alias{print.gmeta.e}
\alias{print.gmeta.m}
\alias{print.gmeta.p}
\alias{print.summary.gmeta.e}
\alias{print.summary.gmeta.m}
\alias{print.summary.gmeta.p}
